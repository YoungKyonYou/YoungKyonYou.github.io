        <span class="nc">ClubAuthMemberDTO</span> <span class="n">authMember</span><span class="o">=(</span><span class="nc">ClubAuthMemberDTO</span><span class="o">)</span> <span class="n">authentication</span><span class="o">.</span><span class="na">getPrincipal</span><span class="o">();</span>
        
        
