        <span class="kt">boolean</span> <span class="n">passwordResult</span><span class="o">=</span><span class="n">passwordEncoder</span><span class="o">.</span><span class="na">matches</span><span class="o">(</span><span class="s">"1111"</span><span class="o">,</span><span class="n">authMember</span><span class="o">.</span><span class="na">getPassword</span><span class="o">());</span>
        
        <span class="c1">//ì†Œì…œ ë¡œê·¸ì¸ì´ê³  íŒ¨ìŠ¤ì›Œë“œê°€ 1111ì¸ ê²½ìš° localhost:8080/member/modifyë¡œ ë¦¬ë‹¤ì´ë ‰íŠ¸ ëœë‹¤.</span>
        <span class="k">if</span><span class="o">(</span><span class="n">fromSocial</span> <span class="o">&amp;&amp;</span> <span class="n">passwordResult</span><span class="o">){</span>
            <span class="n">redirectStrategy</span><span class="o">.</span><span class="na">sendRedirect</span><span class="o">(</span><span class="n">request</span><span class="o">,</span><span class="n">response</span><span class="o">,</span> <span class="s">"/member/modify?from=social"</span><span class="o">);</span>
        <span class="o">}</span>
    <span class="o">}</span>
