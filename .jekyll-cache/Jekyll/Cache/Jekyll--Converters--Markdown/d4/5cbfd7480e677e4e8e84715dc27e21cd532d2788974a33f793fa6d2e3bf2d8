		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"====DrawingEditor use line shape.===="</span><span class="o">);</span>
		<span class="n">drawingEitor</span><span class="o">.</span><span class="na">useShape</span><span class="o">(</span><span class="n">lineShape</span><span class="o">);</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">();</span>

		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"====DrawingEditor use text shape.===="</span><span class="o">);</span>
		<span class="n">drawingEitor</span><span class="o">.</span><span class="na">useShape</span><span class="o">(</span><span class="n">textShape</span><span class="o">);</span>
	<span class="o">}</span>
</code></pre></div></div>

